{{!-- notes-index.handlebars --}}

<h1>Notes</h1>

<div class="row">
    {{#each notes}}
    <div class="col-xm-3">
        <h2>{{this.title}}</h2>
        <ul>
            <sub>{{this.date}}</sub>
            <sub>{{this.time}}</sub>
        </ul>
        <p>{{this.body}}</p>
    </div>
    {{/each}}
</div>
